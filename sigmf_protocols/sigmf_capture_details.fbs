
namespace capture_details;

table Global {
    // `capture_details` does not extend `global`
}

table Capture {
    acq_scale_factor:double=null;
    attentuation:double=null;
    acquisition_bandwidth:double=null;
    start_capture:string;
    stop_capture:string;
    source_file:string;
    gain:double=null;
}

table Annotation {
    SNRdB:double=null;
    signal_reference_number:ulong=null;
}

table Descr {
    global:Global;
    annotation:Annotation;
    capture:Capture;
}
